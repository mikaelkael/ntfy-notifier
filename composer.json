    "symfony/http-client": "^5.0|^6.0",
    "symfony/notifier": "^5.0|^6.0",
    "symfony/event-dispatcher-contracts": "^1.10|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.5|~9.5",
    "vimeo/psalm": "^4.29"
