{
  "name": "mikaelkael/ntfy-notifier",
  "type": "symfony-notifier-bridge",
  "description": "Symfony Ntfy Notifier Bridge",
  "keywords": ["ntfy", "notifier"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mickael Perraud",
      "email": "mikaelkael.fr@gmail.com"
    }
  ],
  "require": {
